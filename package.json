{
  "name": "countr-website",
  "private": true,
  "scripts": {
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "lint": "eslint ./src ./functions",
    "serve": "docusaurus serve",
    "start": "npm run serve",
    "test": "tsc",
    "watch": "docusaurus start"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.17",
    "@docusaurus/plugin-client-redirects": "^2.0.0-beta.17",
    "@docusaurus/plugin-content-blog": "^2.0.0-beta.17",
    "@docusaurus/plugin-content-docs": "^2.0.0-beta.17",
    "@docusaurus/plugin-content-pages": "^2.0.0-beta.17",
    "@docusaurus/plugin-google-analytics": "^2.0.0-beta.17",
    "@docusaurus/plugin-sitemap": "^2.0.0-beta.17",
    "@docusaurus/theme-classic": "^2.0.0-beta.17",
    "@mdx-js/react": "^1.6.21",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-player": "^2.10.0",
    "wrangler": "^0.0.22"
  },
  "devDependencies": {
    "@biaw/eslint-config": "^1.0.2",
    "@cloudflare/workers-types": "^3.4.0",
    "@docusaurus/module-type-aliases": "^2.0.0-beta.17",
    "@docusaurus/types": "^2.0.0-beta.17",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/node": "^16.11.26",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "typescript": "^4.6.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "plugin:react/recommended",
      "@biaw"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 13,
      "sourceType": "module"
    }
  }
}
