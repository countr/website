    "discord-api-types": "0.37.60",
    "eslint": "8.51.0",
    "eslint-config-promise": "github:promise/eslint-config",
    "typescript": "5.2.2",
    "wrangler": "3.13.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
